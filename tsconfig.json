{
  "compilerOptions": {
    "plugins": [{ "name": "typescript-tstl-plugin" }],
    "rootDir": "src",
    "outDir": "lib",
    "types": ["ds-types"],
    "lib": ["ES2020"],
    "strict": true,
    "target": "ES2020"
  },
  "include": ["src"],
  "tstl": {
    "luaTarget": "5.1",
    "luaLibImport": "inline",
    "noImplicitSelf": true
  }
}
